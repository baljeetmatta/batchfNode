<html>
    <body>
        <table>
            <tr><th>Name</th><th>Username</th><th>Password</th></tr>

            <% users.forEach((item)=>{%>
                <tr>
                    <td><%=item.name%></td>
                    <td><%=item.username%></td>
                    <td><%=item.password%></td>
                </tr>

            <%})%>
            

        </table>
    </body>
</html>