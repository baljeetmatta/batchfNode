<html>
    <body>
        <div>List of Users</div>
        <table>
            <tr><th>Username</th> <th>Password</th></tr>
            <% list.forEach((item)=>{%>
                <tr>
                    <td><%=item.username%></td>
                    <td><%=item.password%></td>
                </tr>

           <% })%>
        </table>
    </body>
</html>